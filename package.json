{
  "name": "faceless",
  "version": "0.0.0",
  "description": "Converts JSF apps to Web Components",
  "main": "src/faceless.js",
  "bin": "bin/faceless",
  "scripts": {
    "test": "node_modules/.bin/jshint --reporter=node_modules/jshint-stylish/index.js {src,test}/**/*.js bin/* && node_modules/.bin/mocha test/{**,**/*}.js",
    "test:watch": "node_modules/.bin/chokidar \"{src,test}/**/*.js\" \"bin/*\" -c 'npm test'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/talee/faceless.git"
  },
  "keywords": [
    "web",
    "components",
    "jsf",
    "parser",
    "converter"
  ],
  "author": "Thomas Lee <tleehoc@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/talee/faceless/issues"
  },
  "homepage": "https://github.com/talee/faceless#readme",
  "devDependencies": {
    "chokidar-cli": "^0.2.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.0",
    "mocha": "^2.2.5"
  },
  "dependencies": {
    "dom5": "^1.1.0",
    "es6-promise": "^2.3.0",
    "gulp": "^3.9.0",
    "nopt": "^3.0.2",
    "parse5": "^1.4.2"
  }
}
